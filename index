<div class="slider">
<img name="slide" width="100%" height="500px" img/>
	
	<script>
	var i = 0; 			
var images = [];	
var time = 3000;	
	 
images[0] = "bilder/1.jpg";
images[1] = "bilder/2.jpg";
images[2] = "bilder/3.jpg";
images[3] = "bilder/4.jpg";

function changeImg(){
	document.slide.src = images[i];

	
	if(i < images.length - 1){
	
	  i++; 
	} else { 
		
		i = 0;
	}

	setTimeout("changeImg()", time);
}


window.onload=changeImg;
	</script>
	
